import java.util.Scanner; 
class EvenOrOdd{ 
	public static void main(String[] args){ 
		Scanner sc= new Scanner(System.in); 
		System.out.print("Enter Your number - "); 
		int a= sc.nextInt(); 
		if(a%2==0){ 
			System.out.print("Even"); 
		} 
		else { 
			System.out.print("Odd"); 
		} 
	} 
} 



Output:
Enter Your number - 10
Even
