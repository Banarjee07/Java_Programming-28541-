import java.util.Scanner; 
class SmallNum{ 
	public static void main(String[] args){ 
		Scanner sc= new Scanner(System.in); 
		System.out.print("a = "); 
		int a= sc.nextInt(); 
		System.out.print("b = "); 
		int b= sc.nextInt(); 
		if(a<b){ 
			System.out.print("a is small"); 
		} 
		else { 
			System.out.print("b is small"); 
		} 
	} 
} 



Output:
a = 10
b = 7
b is small
