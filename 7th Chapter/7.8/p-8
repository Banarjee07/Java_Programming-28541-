class Student{ 
	int Roll; 
	String Name; 
	float Mark; 
	public Student(){ 
		Roll = 8272; 
		Name = "Shraus"; 
		Mark = 54.6f; 
		System.out.println("Roll is : "+Roll); 
		System.out.println("Name is : "+Name); 
		System.out.println("Mark is : "+Mark); 
	} 
} 
public class construct { 
	public static void main(String[ ] args) { 
		Student s = new Student(); 
	} 
}



Output:
Roll is : 8272
Name is : Shraus
Mark is : 54.6
